   "source": [
    "import numpy as np\n",
    "\n",
    "scores = np.random.randint(50, 101, size=(5, 3))\n",
    "\n",
    "mean_scores = scores.mean(axis=0)\n",
    "\n",
    "centered_scores = scores - mean_scores\n",
    "\n",
    "print(\"Original Scores:\\n\", scores)\n",
    "print(\"\\nCentered Scores:\\n\", centered_scores)\n"
   ]
  }
