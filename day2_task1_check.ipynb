   "source": [
    "# ask for user's name\n",
    "name = input(\"Enter your name: \")\n",
    "\n",
    "# ask for current age\n",
    "age = input(\"Enter your current age: \")\n",
    "\n",
    "# Convert age to integer\n",
    "age = int(age)\n",
    "\n",
    "# Calculate age in 2030 \n",
    "new_age = age + 4\n",
    "\n",
    "print(\"Hey\", name + \", you will be\", new_age, \"years old in 2030!\")"
